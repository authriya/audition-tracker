    "version": 2,
    "alias": "audition-tracker",
    "name": "audition-tracker",
    "routes": [
        { "handle": "filesystem" },
        { "src": "/.*", "dest": "/index.html" }
    ],
    "build": {
        "env": {
            "REACT_APP_API_ENDPOINT": "https://audition-tracker-api.herokuapp.com/api"
        }
    }
  }